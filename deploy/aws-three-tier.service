[Unit]
Description=AWS Three-Tier Deployment Service
After=network.target docker.service

[Service]
Type=oneshot
User=sonali
WorkingDirectory=/home/sonali/aws-three-tier/deploy
ExecStart=/bin/bash /home/sonali/aws-three-tier/deploy/deploy.sh
RemainAfterExit=yes
Restart=no

[Install]
WantedBy=multi-user.target


